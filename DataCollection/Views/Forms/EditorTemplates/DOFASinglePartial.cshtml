@model DataAccess.Entity.DofaInfo

@Html.HiddenFor(m => m.IDNo)
<td align="center" valign="middle"><span class="srno">&nbsp;</span></td>
<td align="center" valign="middle">@Html.TextBoxFor(m => m.empNo, new { @class = "form-control edit-value", type = "number", min = 1, max = Int32.MaxValue, required="true", title="Add text" })</td>
