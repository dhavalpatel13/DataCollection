<td align="center" valign="middle">@Html.TextBoxFor(m => m.empDEPT, new { @class = "form-control edit-value", minLength = 1, required = "true", title = "Add Text" })</td>
<td align="center" valign="middle">@Html.TextBoxFor(m => m.empDOJ, new { @class = "form-control edit-value datepicker", placeholder = "dd-mmm-yyyy", title = "Add Date" })</td>
