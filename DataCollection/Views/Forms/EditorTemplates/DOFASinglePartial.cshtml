<td align="center" valign="middle">@Html.TextBoxFor(m => m.empNation, new { @class = "form-control edit-value", title = "Add text" })</td>
<td align="center" valign="middle">@Html.TextBoxFor(m => m.QualHighCourse, new { @class = "form-control edit-value", title = "Add text" })</td>
